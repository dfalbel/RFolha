<script>
window.folha.temas
	.prepare( '/365-dias-para-as-olimpiadas/', '366 dias para a Olimp&#237;ada' )
	.init( {
		guide: {
			ajax: {
				url: "/365-dias-para-as-olimpiadas/virtual/1/common/season.json"
			}
		}
	} ) ;
